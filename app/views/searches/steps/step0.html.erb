<h1>Search Task</h1>
<br>

<%= form_for @search, :html => { :class => 'form-horizontal' } do |f| %>
  <%= render "shared/error_messages", :object => f.object %>
  <br>
  <%= render "searches/tasks/task0", :f => f %>
  <p>
    <%= f.submit "Next Question", :class => 'btn btn-primary' %>
  </p>
<% end %>