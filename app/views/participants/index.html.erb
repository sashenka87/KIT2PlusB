<h1>Listing participants</h1>

<table>
  <tr>
    <th>Session</th>
    <th>Ip address</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Instructor</th>
    <th>Country</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @participants.each do |participant| %>
  <tr>
    <td><%= participant.session_id %></td>
    <td><%= participant.ip_address %></td>
    <td><%= participant.first_name %></td>
    <td><%= participant.last_name %></td>
    <td><%= participant.instructor %></td>
    <td><%= participant.country %></td>
    <td><%= link_to 'Show', participant %></td>
    <td><%= link_to 'Edit', edit_participant_path(participant) %></td>
    <td><%= link_to 'Destroy', participant, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Participant', new_participant_path %>
